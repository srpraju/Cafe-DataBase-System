<% if (chartType == "stock") { %>
<kendo-stockchart renderAs="<%=renderAs%>">
<% } else { %>
<kendo-chart renderAs="<%=renderAs%>">
<% } %>
<% if (title) { %>
    <kendo-chart-title
        text="<%= capitalize(name) %>"
        position="top"
        align="left">
    </kendo-chart-title>
<% } %>
<% if (legend) { %>
    <kendo-chart-legend
        align="center"
        position="right"
        orientation="vertical"
        [title]="title">
    </kendo-chart-legend>
<% } %>
    <kendo-chart-series>
<% if (chartType == "scatter") { %>
        <kendo-chart-series-item
            type="scatter"
            [data]="seriesData"
            xField="rainfall"
            yField="windSpeed">
        </kendo-chart-series-item>
<% } else { %>
<% for (let item of series) { %>
        <kendo-chart-series-item
            name="<%=capitalize(item)%>"
            categoryField="<%=camelize(category)%>"
            [data]="seriesData"
<% if (chartType == "stock") { %>
            openField="<%=item%>Open"
            closeField="<%=item%>Close"
            lowField="<%=item%>Low"
            highField="<%=item%>High"
            type="candlestick"
<% } else if (chartType == "rangeArea" || chartType == "rangeBar" || chartType == "rangeColumn") { %>
            fromField="<%=camelize(item)%>Min"
            toField="<%=camelize(item)%>Max"
            type="<%=chartType%>"
<% } else if (chartType == "scatter") { %>
            xField="rainfall"
            yField="windSpeed"
            type="<%=chartType%>"
<% } else { %>
            field="<%=camelize(item)%>"
            type="<%=chartType%>"
<% } %>
            >
        </kendo-chart-series-item>
<% } %>
<% } %>
    </kendo-chart-series>
<% if (chartType == "scatter") { %>
    <kendo-chart-x-axis>
        <kendo-chart-x-axis-item [title]="{ text: 'Wind Speed [km/h]' }">
        </kendo-chart-x-axis-item>
    </kendo-chart-x-axis>
    <kendo-chart-y-axis>
        <kendo-chart-y-axis-item [title]="{ text: 'Rainfall [mm]' }">
        </kendo-chart-y-axis-item>
    </kendo-chart-y-axis>
<% } else { %>
    <kendo-chart-category-axis>
        <kendo-chart-category-axis-item name="category" type="<% if (time) { %>date<% } else { %>category<% } %>">
            <kendo-chart-category-axis-item-title text="<%=capitalize(category)%>"></kendo-chart-category-axis-item-title>
        </kendo-chart-category-axis-item>
    </kendo-chart-category-axis>

    <kendo-chart-value-axis>
        <kendo-chart-value-axis-item type="<% if (log) { %>log<% } else { %>numeric<% } %>">
            <kendo-chart-value-axis-item-title text="<%=capitalize(value)%>"></kendo-chart-value-axis-item-title>
        </kendo-chart-value-axis-item>
    </kendo-chart-value-axis>
<% } %>
<% if (chartType == "stock" && navigator) { %>
    <kendo-chart-navigator>
        <kendo-chart-navigator-select
            [from]="navigatorFrom"
            [to]="navigatorTo">
        </kendo-chart-navigator-select>
        <kendo-chart-navigator-series>
<% for (let item of series) { %>
            <kendo-chart-navigator-series-item
                categoryField="<%=camelize(category)%>"
                field="<%=item%>Close"
                [data]="seriesData"
                name="<%=capitalize(item)%>"
                type="line">
            </kendo-chart-navigator-series-item>
<% } %>
        </kendo-chart-navigator-series>
    </kendo-chart-navigator>
<% } %>
<% if (chartType == "stock") { %>
</kendo-stockchart>
<% } else { %>
</kendo-chart>
<% } %>